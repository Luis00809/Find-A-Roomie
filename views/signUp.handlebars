<main class="container ">

      <h2>Sign Up: </h2>
      <div class="dropdown bg-primary">
        <form class="px-4 py-3">
          <div class="mb-3">
            <label for="exampleDropdownFormEmail1" class="form-label">Email address</label>
            <input type="email" class="form-control" id="email" placeholder="email@example.com">
          </div>
          <div class="mb-3">
            <label for="exampleDropdownFormPassword1" class="form-label">Password</label>
            <input type="password" class="form-control" id="password" placeholder="Password">
          </div>
          <div class="mb-3">
            <label for="exampleDropdownFormPassword1" class="form-label">First Name:</label>
            <input type="username" class="form-control" id="firstName" placeholder="Fist Name">
          </div>
          <div class="mb-3">
            <label for="exampleDropdownFormPassword1" class="form-label">Last Name</label>
            <input type="username" class="form-control" id="lastName" placeholder="Last Name">
          </div>
          <div class="mb-3">
            <label for="exampleDropdownFormPassword1" class="form-label">Gender</label>
            <select id="gender" class="form-select" aria-label="Default select example">
              <option selected>Select</option>
              <option value="Male">Male</option>
              <option value="Female">Female</option>
              <option value="Other">Other</option>
            </select>
          </div>
          <button type="button" id="signUpBTN" class="btn btn-dark">Sign up</button>
        </form>
        <div class="dropdown-divider"></div>
        <a class="dropdown-item bg-primary" href="/login">Already have an account? login here</a>
      </div>
      
      </main>

<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-C6RzsynM9kWDrMNeT87bh95OGNyZPhcTNXj1NW7RuBCsyN/o0jlpcV8Qyq46cDfL" crossorigin="anonymous"></script>
